<UserControl x:Class="FacultyManager.View.Register"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FacultyManager.View"
        mc:Ignorable="d"
       >
    <Grid>
        <Button Width="80" HorizontalAlignment="Left" Command="{Binding NavigateLoginCommand}" Style="{StaticResource ButtonTemplate}" BorderBrush="#fff" Grid.Column="3" Grid.Row="5" Content="Login" FontWeight="UltraBold"/>
    </Grid>
</UserControl>
